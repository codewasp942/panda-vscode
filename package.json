{
    "name": "panda-vscode",
    "displayName": "panda-vscode",
    "description": "Panda vscode support. Panda page is at https://gitee.com/panda-dev/panda.",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.57.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "panda",
            "aliases": ["panda", "panda"],
            "extensions": [".pd"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "panda",
            "scopeName": "source.panda",
            "path": "./syntaxes/panda.tmLanguage.json"
        }]
    },
    "configurationDefaults": {
        "[languageId]": {
          "editor.semanticHighlighting.enabled": true
        }
      }
}